<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.albk.k8s.demo.persistent.GoodsDAO">
    <select id="findById" parameterType="long" resultType="com.albk.k8s.demo.entity.Goods">
        select * from t_goods where goods_id = #{value}
    </select>
</mapper>
